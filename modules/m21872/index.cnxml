							<m:mfrac>
								<m:mrow>
									<m:msup>
										<m:mrow>
											<m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>2</m:mn><m:mi>y</m:mi><m:mo stretchy="false">)</m:mo>
										</m:mrow>
										<m:mn>5</m:mn>
									</m:msup>
									<m:msup>
										<m:mrow>
											<m:mo stretchy="false">(</m:mo><m:mn>3</m:mn><m:mi>x</m:mi><m:mo>−</m:mo><m:mn>1</m:mn><m:mo stretchy="false">)</m:mo>
										</m:mrow>
										<m:mn>7</m:mn>
									</m:msup>
								</m:mrow>
								<m:mrow>
									<m:msup>
										<m:mrow>
											<m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>2</m:mn><m:mi>y</m:mi><m:mo stretchy="false">)</m:mo>
										</m:mrow>
										<m:mn>3</m:mn>
									</m:msup>
									<m:msup>
										<m:mrow>
											<m:mo stretchy="false">(</m:mo><m:mn>3</m:mn><m:mi>x</m:mi><m:mo>−</m:mo><m:mn>1</m:mn><m:mo stretchy="false">)</m:mo>
										</m:mrow>
										<m:mn>6</m:mn>
									</m:msup>
								</m:mrow>
							</m:mfrac>
						</m:mrow>
						<m:annotation encoding="MathType-MTEF">
						</m:annotation>
					</m:semantics>
				</m:math>.  </para>
		</problem><solution id="fs-id4245905"><para id="fs-id4245906"><m:math>
					<m:semantics>
						<m:mrow>
